      branch="main"
      tag="7594374942b349d22b241144bf2aeb87280fd961"
      module="libjxl/libjxl.git"
      checkoutdir="libjxl"/>
    <dependencies>
      <dep package="libbrotli"/>
    </dependencies>
  </cmake>

